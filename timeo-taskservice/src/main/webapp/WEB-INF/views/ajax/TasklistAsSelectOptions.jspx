<select name="task" id="activityFormTask" size="1"
    xmlns:c="http://java.sun.com/jsp/jstl/core">
    <c:forEach items="${tasks}" var="task" varStatus="varStatus">
        <option value="${task.id}">${task.name}</option>
    </c:forEach>
</select>
